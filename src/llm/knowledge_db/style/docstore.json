046\u3075\u3056\u3051\u305f\u540d\u4e57\u308a\u304c\u3067\u304d\u308b\u306e\u304b\u3057\u3089\u3002\r\n\u30a2\u30ca\u30b9\u30bf\u30b7\u30a2\uff1a\u9006\u306b\u898b\u7fd2\u3044\u305f\u3044\u308f\u306d\u3002\r\n\uff1f\uff1f\uff1f\uff1a\u4e00\u5207\u306e\u622f\u8a00\u306f\u306a\u304f\u3002\r\n\uff1f\uff1f\uff1f\uff1a\u79c1\u306f\u30e9\u30e0\u30d0\u30fc\u3067\u3042\u308a\u3001\u30c6\u30a3\u30ed\u30fc\u30c3\u30bf\u30de\u30fc\u3067\u3042\u308a\u3001 \u3059\u3079\u3066\u306e\u3001\u30a4\u30f3\u30c9\u30e9\u304c\u597d\u3080\u7f8e\u3057\u304d\u5973\u306b\u3057\u3066 \u3042\u308b\u3044\u306f\u3001\u4ed6\u306e\u5168\u3066\u3067\u3082\u3042\u308b\u3082\u306e\u3002\r\n\uff1f1\uff1a\u2026\u2026\uff1f\r\n\uff1f\uff01\r\n\u30a2\u30eb\u30b8\u30e5\u30ca\uff1a\u540d\u306e\u51fa\u305f\u4e8c\u4eba\u306f\u3001\u30a4\u30f3\u30c9\u30e9\u306e\u795e\u8a71\u306b\u95a2\u308f\u308b\u5973\u3067\u3059\u3002 \u5171\u306b\u305d\u306e\u7f8e\u3057\u3055\u304c\u8b33\u308f\u308c\u308b\u5b58\u5728\u3002\r\n\u30a2\u30eb\u30b8\u30e5\u30ca\uff1a\u3057\u304b\u3057\u3001\u79c1\u306b\u3082\u308f\u304b\u308b\u306e\u306f\u305d\u308c\u3060\u3051\u3067\u3059\u3002 \u4ed6\u306e\u8a00\u8449\u306e\u610f\u5473\u306f\r\n\uff1f\uff1f\uff1f\uff1a\u305d\u3046\u3001\u79c1\u306f\u3001 \u30a4\u30f3\u30c9\u30e9\u304c\u597d\u3080\u5973\u3068\u3044\u3046\u5f62\u305d\u306e\u3082\u306e\u3002\r\n\uff1f\uff1f\uff1f\uff1a\u79c1\u306f\u8ab0\u3067\u3082\u306a\u304f\u3002 \u540c\u6642\u306b\u3001\u8ab0\u3067\u3082\u3042\u308b\u3002\r\n\uff1f\uff1f\uff1f\uff1a\u8a00\u3044\u63db\u3048\u3088\u3046\u304b\uff1f \u79c1\u306f\u2026\u2026\r\n\uff1f\uff1f\uff1f\uff1a\u3069\u306e\u3088\u3046\u306a\u5f62\u3067\u3082\u3042\u308b\u3001 \u4e07\u7269\u3060\u3002\r\n\u30a4\u30f3\u30c9\u30e9\uff1a\u8cb4\u69d8\u3001\u3082\u3057\u3084\u2026\u2026 \u4e07\u7269\u306e\u5f62\u2026\u2026\uff01\r\n\uff1f\uff1f\uff1f\uff1a\u3046\u3075\u3075\u3001\u3042\u306f\u306f\u306f\uff01 \u3082\u3046\u4e00\u3064\u306e\u540d\u3092\u77e5\u3089\u306c\u308f\u3051\u3067\u3082\u3042\u308b\u307e\u3044\u306b\uff01\r\n\uff1f\uff1f\uff1f\uff1a\u3084\u306f\u308a\u611a\u304b\u3088\u3001\u304a\u307e\u3048\u3060\u3051\u304c\u898b\u3066\u3044\u305f\u3068\u3044\u3046\u306e\u306b\uff01 \u4e09\u3064\u306e\u982d\u3092\u6301\u3064\u3082\u306e\u304c\u3053\u3053\u306b\u3044\u305f\u3053\u3068\u3092\uff01\r\n\uff1f\uff1f\uff1f\uff1a\u5973\u305f\u308b\u982d\u3001\u5c0f\u3055\u304d\u982d\u3001\u6ce1\u306e\u4e2d\u306e\u982d\uff01 \u3055\u3042\u3001\u4e00\u3064\u3068\u306a\u308a\u76ee\u899a\u3081\u308b\u3068\u304d\u3060\uff01\r\n\u30f4\u30ea\u30c8\u30e9\uff1a\u304d\u3001\u3072\u3001\u3072\uff01 \u7121\u7406\u3058\u3083\u3068\u601d\u3046\u304c\u306e\uff01\r\n\u30f4\u30ea\u30c8\u30e9\uff1a\u529b\u3092\u5931\u3063\u305f\u795e\u3005\u304c\u6700\u5f8c\u306b\u983c\u308a\u3001 \u305d\u3057\u3066\u6050\u308c\u305f\u970a\u9152\u306e\u529b\u306b\u52dd\u3066\u308b\u308f\u3051\u304c\u3042\u308b\u307e\u3044\uff01\r\n\u30f4\u30a1\u30b8\u30e5\u30e9=spot[G,F]\uff1a\u3067\u306f\u3001\u30a4\u30f3\u30c9\u30e9\u3055\u307e\uff1f\r\n\u30a4\u30f3\u30c9\u30e9\uff1a\u30aa\u30ec\u306f\u3001\u4eca\u3053\u3053\u306b\uff01 \u795e\u3005\u306e\u738b\u3068\u3057\u3066\u306e\u30aa\u30ec\u3092\u5fd8\u308c\u308b\uff01\r\n\uff1f1\uff1a\u4e09\u4eba\u304c\u5408\u4f53\u3057\u3066\u2026\u2026\r\n\uff1f2\uff1a\u3042\u306e\u30f4\u30ea\u30c8\u30e9\u306b\u306a\u3063\u305f\u2026\u2026\uff1f\r\n\uff1f\uff01\r\n\u30a2\u30eb\u30b8\u30e5\u30ca\uff1a\u305d\u306e\u3088\u3046\u3067\u3059\u3002\u4eca\u307e\u3067\u540c\u884c\u3057\u3066\u3044\u305f\u5b50\u4f9b\u306e\u30f4\u30ea\u30c8\u30e9\u3001 \u6ce1\u306e\u4e2d\u306b\u5c01\u3058\u3089\u308c\u3066\u3044\u305f\u30f4\u30ea\u30c8\u30e9\u3001\u305d\u3057\u3066\u5148\u7a0b\u306e\u8b0e\u306e\u5973\u3002\r\n\u30a2\u30eb\u30b8\u30e5\u30ca\uff1a\u305d\u308c\u3089\u304c\u5408\u308f\u3055\u3063\u305f\u3082\u306e\u304c \u3042\u306e\u30f4\u30ea\u30c8\u30e9\u304b\u3068\u3002\r\n\u30e9\u30fc\u30de\uff1a\u7269\u8a00\u3044\u304b\u3089\u3057\u3066\u3001\u3042\u308c\u3082\u30ab\u3